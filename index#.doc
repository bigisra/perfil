    <link href="https://fonts.googleapis.com/css2?family=Plus+Jakarta+Sans:wght@300;400;600;700&display=swap"
        rel="stylesheet">
    <style>
        :root {
            --bege-fundo: #f5f2ed;
            /* Bege areia bem clarinho */
            --bege-card: #ffffff;
            --verde-primario: #3d5a44;
            /* Verde Oliva sofisticado */
            --verde-claro: #e9ede9;
            --texto-dark: #2d3436;
            --texto-light: #636e72;
        }

        * {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
            scroll-behavior: smooth;
        }

        body {
            font-family: 'Plus Jakarta Sans', sans-serif;
            background-color: var(--bege-fundo);
            color: var(--texto-dark);
        }

        /* Navbar elegante */
        nav {
            position: fixed;
            top: 20px;
            left: 50%;
            transform: translateX(-50%);
            width: 90%;
            max-width: 600px;
            background: rgba(255, 255, 255, 0.8);
            backdrop-filter: blur(10px);
            padding: 12px 30px;
            border-radius: 100px;
            display: flex;
            justify-content: space-between;
            align-items: center;
            box-shadow: 0 10px 30px rgba(0, 0, 0, 0.05);
            z-index: 1000;
            border: 1px solid rgba(255, 255, 255, 0.3);
        }

        nav a {
            text-decoration: none;
            color: var(--texto-dark);
            font-weight: 600;
            font-size: 0.9rem;
            transition: 0.3s;
        }

        nav a:hover {
            color: var(--verde-primario);
        }

        /* Hero Section */
        .container {
            max-width: 1000px;
            margin: 0 auto;
            padding: 120px 20px 60px;
        }

        .profile-card {
            background: var(--bege-card);
            border-radius: 30px;
            padding: 50px;
            display: flex;
            align-items: center;
            gap: 40px;
            box-shadow: 0 20px 40px rgba(0, 0, 0, 0.02);
            margin-bottom: 30px;
        }

        .profile-img {
            width: 200px;
            height: 200px;
            border-radius: 25px;
            object-fit: cover;
            background-color: var(--verde-claro);
            transform: rotate(-3deg);
            transition: 0.3s;
        }

        .profile-card:hover .profile-img {
            transform: rotate(0deg);
        }

        .profile-info h1 {
            font-size: 2.8rem;
            color: var(--verde-primario);
            line-height: 1.1;
            margin-bottom: 10px;
        }

        .profile-info p {
            color: var(--texto-light);
            font-size: 1.1rem;
        }

        /* Seções de Conteúdo */
        .grid-content {
            display: grid;
            grid-template-columns: 1.5fr 1fr;
            gap: 30px;
        }

        section {
            background: var(--bege-card);
            padding: 40px;
            border-radius: 30px;
            box-shadow: 0 20px 40px rgba(0, 0, 0, 0.02);
        }

        h2 {
            font-size: 1.4rem;
            margin-bottom: 20px;
            color: var(--verde-primario);
            display: flex;
            align-items: center;
            gap: 10px;
        }

        /* Habilidades Style */
        .skills-tag {
            display: flex;
            flex-wrap: wrap;
            gap: 10px;
        }

        .tag {
            background: var(--verde-claro);
            color: var(--verde-primario);
            padding: 10px 20px;
            border-radius: 12px;
            font-size: 0.85rem;
            font-weight: 700;
        }

        .btn-contact {
            display: inline-block;
            background: var(--verde-primario);
            color: white;
            padding: 15px 35px;
            border-radius: 15px;
            text-decoration: none;
            font-weight: 600;
            margin-top: 20px;
            transition: 0.3s;
        }

        .btn-contact:hover {
            transform: translateY(-3px);
            box-shadow: 0 10px 20px rgba(61, 90, 68, 0.2);
        }

        /* Responsividade */
        @media (max-width: 850px) {
            .profile-card {
                flex-direction: column;
                text-align: center;
                padding: 30px;
            }

            .grid-content {
                grid-template-columns: 1fr;
            }

            .profile-img {
                transform: rotate(0);
                width: 150px;
                height: 150px;
            }
        }
    </style>
</head>

